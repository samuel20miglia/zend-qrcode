{
  "name" : "zendframework/zend-qrcode",
  "description" : "generic component to generate qrcodes",
  "require" : {
    "php" : "^5.5 || ^7.0",
    "zendframework/zend-stdlib" : "^2.7 || ^3.0",
    "zendframework/zend-validator" : "^2.6.1",
    "zendframework/zend-servicemanager" : "^2.7.5 || ^3.0.3"
  },
  "require-dev" : {
    "zendframework/zend-config" : "^2.6",
    "zendframework/zendpdf" : "*",
    "squizlabs/php_codesniffer" : "2.7.1",
    "fabpot/php-cs-fixer": "1.7.*"
  },
  "license" : "BSD-3-Clause",
  "keywords" : [ "zf", "qrcode", "zf qrcode" ],
  "autoload-dev" : {
    "psr-4" : {
      "ZendTest\\Qrcode\\" : "test/"
    }
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "2.6-dev",
      "dev-develop" : "2.7-dev"
    }
  },
  "autoload" : {
    "psr-4" : {
      "Zend\\Qrcode\\" : "src/"
    }
  },
  "suggest" : {
    "zendframework/zendpdf" : "ZendPdf component"
  },
  "scripts" : {
    "check" : [ ],
    "cs-check" : "phpcs",
    "cs-fix" : "phpcbf",
    "test" : "phpunit",
    "test-coverage" : "phpunit --coverage-clover clover.xml"
  },
  "homepage" : "https://github.com/zendframework/zend-qrcode",
  "minimum-stability": "dev",
  "prefer-stable": true
}